{
    "name": "memochou1993/lexicon-api-php-client",
    "description": "Lexicon API Client Library for PHP",
    "type": "library",
    "keywords": [
        "localization"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Memo Chou",
            "email": "memochou1993@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "vlucas/phpdotenv": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "MemoChou1993\\Lexicon\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MemoChou1993\\Lexicon\\Tests\\": "tests/"
        }
    }
}
